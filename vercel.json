{
  "version": 2,
  "builds": [
    { 
      "src": "api/**/*.ts", 
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["generated/**/*"]
      }
    }
  ],
  "routes": [
    { "src": "/api/login/location/store", "dest": "/api/login-location-store.ts" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 